{
  "name": "lifion-kinesis",
  "version": "1.1.1",
  "description": "Lifion client for Amazon Kinesis Data streams",
  "keywords": [
    "adp",
    "amazon",
    "api",
    "aws",
    "big data",
    "client",
    "core",
    "kinesis",
    "lifion"
  ],
  "author": "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
  "maintainers": [
    "Chen Doron <Chen.Doron@ADP.com>",
    "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
    "Jenny Eckstein <Jenny.Eckstein@ADP.com>",
    "Mackenzie Turner <turner.mackenzie.m@gmail.com>",
    "Zaid Masud <Zaid.Masud@ADP.com>"
  ],
  "contributors": [
    "Simon Gellis <Simon.Gellis@ADP.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifion/lifion-kinesis.git"
  },
  "bugs": {
    "url": "https://github.com/lifion/lifion-kinesis/issues"
  },
  "homepage": "https://github.com/lifion/lifion-kinesis#readme",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build-docs": "jsdoc2md -t ./templates/README.hbs ./lib/*.js > ./README.md && git add ./README.md",
    "build-docs-watch": "npm-watch build-docs",
    "eslint": "eslint . --ext .js,.json --ignore-pattern='!.*.*'",
    "format": "prettier --write '**/*.{md,js,json}' '!coverage/**/*.{js,json}'",
    "jest": "jest -c ./.jest.json",
    "prepare": "check-engines",
    "test": "npm run jest -- --ci --runInBand && codecov",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "async-retry": "^1.2.3",
    "aws-sdk": "^2.535.0",
    "aws4": "^1.8.0",
    "fast-deep-equal": "^2.0.1",
    "got": "^9.6.0",
    "is-retry-allowed": "^1.2.0",
    "lifion-aws-event-stream": "^1.0.5",
    "lru-cache": "^5.1.1",
    "lzutf8": "^0.5.5",
    "project-name": "^1.0.0",
    "short-uuid": "^3.1.1"
  },
  "devDependencies": {
    "auto-changelog": "^1.16.1",
    "chalk": "^2.4.2",
    "chance": "^1.1.0",
    "check-engines": "^1.5.0",
    "codecov": "^3.6.1",
    "eslint": "^6.4.0",
    "eslint-config-lifion": "^1.3.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.2",
    "lint-staged": "^9.3.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "semver": "^6.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-docs && lint-staged",
      "pre-push": "npm run eslint && npm run jest"
    }
  },
  "watch": {
    "build-docs": {
      "patterns": [
        "lib"
      ]
    }
  },
  "@lifion/core-commons": {
    "template": "public",
    "updated": "2019-08-27T00:07:22.435Z",
    "version": "2.3.3"
  }
}
